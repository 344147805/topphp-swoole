{
    "name": "topphp/topphp-swoole",
    "type": "library",
    "description": "基于swoole的组件",
    "keywords": [
        "topphp",
        "topphp-swoole"
    ],
    "homepage": "https://github.com/topphp/topphp-swoole",
    "license": "MIT",
    "authors": [
        {
            "name": "topphp",
            "email": "sleep@kaituocn.com",
            "homepage": "https://github.com/topphp",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "topthink/framework": "^6.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.20",
        "squizlabs/php_codesniffer": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "topphp\\swoole\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "topphp\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "post-root-package-install": []
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.1-dev"
        },
        "think": {
            "services": [
                "topphp\\swoole\\services\\Service"
            ],
            "config": {
                "topphpServer": "config/topphpServer.php"
            }
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer"
        }
    }
}
